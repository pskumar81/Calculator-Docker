{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Calculator.Client": "Debug",
      "Microsoft.Extensions.Hosting": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "CalculatorService": {
    "ServerUrl": "http://localhost:5002",
    "TimeoutSeconds": 30,
    "RetryCount": 3,
    "RetryDelayMs": 1000
  },
  "Azure": {
    "VM": {
      "Name": "calculator-client-vm",
      "ResourceGroup": "calculator-grpc-rg",
      "Region": "East US",
      "PrivateIP": "10.0.1.5",
      "PublicDNS": "calculator-client.eastus.cloudapp.azure.com"
    },
    "Server": {
      "PrivateIP": "10.0.1.4",
      "PublicDNS": "calculator-server.eastus.cloudapp.azure.com",
      "Port": 5002
    },
    "ApplicationInsights": {
      "InstrumentationKey": "",
      "EnableDependencyTrackingTelemetryModule": true
    }
  }
}